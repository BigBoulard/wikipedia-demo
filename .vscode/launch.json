{
  // See https://code.visualstudio.com/docs/editor/debugging#_launchjson-attributes
  // and variables here https://code.visualstudio.com/docs/editor/variables-reference
  "version": "0.2.0",
  "configurations": [
    {
      "name": "wiki",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "env": {
        "LOG_LEVEL": "1",
        "APP_ENV": "development"
      },
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}"
    }
  ]
}
